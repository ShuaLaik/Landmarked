{
  "name": "landmarked",
  "version": "1.0.0",
  "description": "An online travel journal",
  "main": "app.js",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShuaLaik/Landmarked.git"
  },
  "server": "nodemon app.js --ignore client",
  "keywords": [
    "travel"
  ],
  
  "author": "Josh, Mack, Izy, Joe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShuaLaik/Landmarked/issues"
  },
  "homepage": "https://github.com/ShuaLaik/Landmarked#readme",
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.20",
    "aws-sdk": "^2.1058.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "multer": "^1.4.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "react-scripts": "^5.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "mini-css-extract-plugin": "2.4.5",
    "nodemon": "^2.0.15"
  }
}
